rting(Highcharts);\nAccessibility(Highcharts);\nSeriesLabel(Highcharts);\nHighContrastLight(Highcharts);\n","scripts":["https://code.highcharts.com/11.0.0/highcharts.js","https://code.highcharts.com/11.0.0/modules/exporting.js","https://code.highcharts.com/11.0.0/modules/accessibility.js","https://code.highcharts.com/11.0.0/modules/series-label.js","https://code.highcharts.com/11.0.0/themes/high-contrast-light.js"]}